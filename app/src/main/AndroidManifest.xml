            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PreLevelOneActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LevelOneActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".About" />
        <activity android:name=".Profil_Samuel"
            android:screenOrientation="portrait" />
        <activity android:name=".Profil_Nataya"
            android:screenOrientation="portrait" />
        <activity android:name=".Profil_Wisnu"
            android:screenOrientation="portrait" />
        <activity android:name=".Profil_Neven"
            android:screenOrientation="portrait" />
        <activity
            android:name=".PreLevelTwoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LevelTwoActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".GameOverActivity"></activity>
        <activity android:name=".PreLevelThreeActivity"
            android:screenOrientation="portrait"></activity>
